{
    "name": "deepskylog/laravel-astronomylibrary",
    "description": "A library to execute astronomical calculations",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "The DeepskyLog Team",
            "email": "developers@DeepskyLog.be",
            "homepage": "https://www.deepskylog.org/"
        }
    ],
    "homepage": "https://github.com/DeepskyLog/laravel-astronomylibrary",
    "keywords": ["Laravel", "laravel-astronomylibrary"],
    "require": {
        "illuminate/support": "~5|~6|~7"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.1",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0",
        "cedx/coveralls": "^12.0"
    },
    "autoload": {
        "psr-4": {
            "deepskylog\\laravel-astronomylibrary\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "deepskylog\\laravel-astronomylibrary\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "deepskylog\\laravel-astronomylibrary\\laravel-astronomylibraryServiceProvider"
            ],
            "aliases": {
                "laravel-astronomylibrary": "deepskylog\\laravel-astronomylibrary\\Facades\\laravel-astronomylibrary"
            }
        }
    }
}
